<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.javalearn.infrastructure.dao.StrategyDao">

    <resultMap id="dataMap" type="org.javalearn.infrastructure.dao.po.Strategy">
        <id column="id" property="id"/>
        <result column="strategy_id" property="strategyId"/>
        <result column="strategy_desc" property="strategyDesc"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <select id="getAll" resultMap="dataMap">
        SELECT id,strategy_id,strategy_desc,create_time,update_time
        FROM strategy
        LIMIT 10;
    </select>

</mapper>